pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo  'Finish Building'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo 'Finish Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                echo 'Finish Deploying'
            }
        }
    }
}